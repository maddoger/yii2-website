{
    "name": "maddoger/yii2-website",
    "type": "yii2-extension",
    "description": "Website manager extension for Yii2.",
    "authors": [
        {
            "name": "Vitaliy Syrchikov",
            "email": "maddoger@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-jui": "*",
        "maddoger/yii2-core": "*",
        "maddoger/yii2-select2": "*"
    },
    "autoload": {
        "psr-4": {
            "maddoger\\website\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}